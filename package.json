  "main": "index.js",
  "bin": {
    "www": "./bin/www"
  },
  "scripts": {
    "www": "node ./bin/www",
    "start": "better-npm-run start",
    "log:dev": "better-npm-run log:dev",
    "log:pro": "better-npm-run log:pro",
    "dev": "better-npm-run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": ""
  },
  "betterScripts": {
    "start": {
      "command": "gulp",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "gulp dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "log:dev": {
      "command": "cross-env NODE_ENV=development node server.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "log:pro": {
      "command": "cross-env NODE_ENV=production node server.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
